class calculator{
    int a=10;
    int n=20;

    calculator(){

    }
    int add(int a ,int b){
      return a+b;
     
    }
    void multiply(int a,int b){
        int multiply=a*b;
        System.out.println(multiply);

    }

}
public class cal{
    public static void main(String args[]){
        calculator cal1=new calculator();
        System.out.println(cal1.add(10,20));
        cal1.multiply(10,20);
        
    }
}
3.
class MyClass {
    int value;

   
   public MyClass() {
       value = 0;
   }

   // Parameterized constructor
   public MyClass(int val) {
       value = val;
   }

   // Method to display the value
   public void display() {
       System.out.println("Value: " + value);
   }

   // Overloaded method to display the sum of two values
   public void display(int num1, int num2) {
       System.out.println("Sum: " + (num1 + num2));
   }

   // Overloaded method to display the product of two values
   public void display(double num1, double num2) {
       System.out.println("Product: " + (num1 * num2));
   }
}

public class overloading_eg {
   public static void main(String[] args) {
       // Using constructors
       MyClass obj1 = new MyClass(); 
       MyClass obj2 = new MyClass(4); 

       // Using methods
       obj1.display(); 
       obj2.display(); 
       obj1.display(3, 2); 
       obj2.display(2.5, 7.9);
   }
}
